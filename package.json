    "postinstall": "npm run build",
    "dev:webpack": "webpack-dev-server --config ./webpack.config.dev.js --watch",
    "webpack": "webpack -d --config ./webpack.config.dev.js --watch",
    "start:dev": "npm-run-all --parallel webpack dev:start"
