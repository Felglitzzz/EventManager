{
  "name": "eventmanager",
  "version": "1.0.0",
  "description": "EventManager is a light-weight app that allows users to manage and book centers for their events.",
  "main": "index.js",
  "scripts": {
    "build-server": "babel -d ./dist-server --copy-files ./server -s",
    "build-client": "webpack -p --config webpack.config.prod.js",
    "build:dev": "npm run build-server && npm run webpack",
    "clean": "rimraf dist-server && rimraf dist-client",
    "lint": "eslint --fix index",
    "seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "migrate:test": "sequelize db:migrate --env test",
    "coveralls:report": "nyc report --reporter=text-lcov | coveralls",
    "server-test": "cross-env NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --timeout 20000 --require babel-polyfill --compilers js:babel-register server/test/index.spec.js",
    "client-test": "jest && codecov",
    "test": "npm run server-test && npm run client-test",
    "heroku-postbuild": "npm run build-server && npm run build-client",
    "webpack": "webpack -d --config ./webpack.config.dev.js",
    "webpack:watch": "webpack -d --config ./webpack.config.dev.js --watch",
    "start": "node dist-server/index.js",
    "dev:start": "nodemon --exec babel-node -- ./dist-server/index.js",
    "dev": "nodemon --exec babel-node -- ./server/index.js",
    "start:dev": "npm run clean && npm run build:dev && npm run dev:start",
    "start:dev:watch": "npm-run-all --parallel dev webpack:watch",
    "update-snapshot": "jest -u",
    "e2e-tests": "nightwatch",
    "client-test-dev": "jest --coverage"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./client/src/__tests__/setupTests.js",
    "testMatch": [
      "**/**/__tests__/**/*.spec.js?(x)",
      "**/**/__tests__/?(*.)(spec|test).js?(x)"
    ],
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/**/*.js?(x)",
      "!client/src/index.js",
      "!client/src/routes.js",
      "!client/src/store/**"
    ]
  },
  "keywords": [
    "event"
  ],
  "author": "Amande Olaoluwa Felix",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eventsource-polyfill": "^0.9.6",
    "faker": "^4.1.0",
    "file-loader": "^1.1.6",
    "jest": "^23.0.1",
    "jest-enzyme": "^6.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-local-storage": "^1.0.5",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "selenium-standalone": "^6.15.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.2",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "aos": "^2.2.0",
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0-beta",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chai-json-equal": "0.0.1",
    "codecov": "^3.0.2",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mocha": "^3.5.3",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.8.3",
    "nodemailer": "^4.6.4",
    "npm-run-all": "^4.1.2",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.0.1",
    "preset": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-md-spinner": "^0.2.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.23.1",
    "swagger-ui": "^3.6.0",
    "swagger-ui-express": "^2.0.11",
    "sweetalert": "^2.1.0",
    "toastr": "^2.1.4",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "validator": "^9.2.0",
    "winston": "^2.4.2"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.5.1"
  }
}
